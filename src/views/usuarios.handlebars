<h1>usuarios</h1>

<table class="table">
    <tr>
      <th>ID</th>
      <th>Usuario</th>
      <th>Email</th>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Ciudad</th>
      <th>Tel√©fono</th>
      <th>Estado</th>
    </tr>

    {{#each users}}
    <tr>
      <td>{{this.id_usuario}}</td>
      <td>{{this.username}}</td>
      <td>{{this.mail}}</td>
      <td>{{this.name}}</td>
      <td>{{this.surname}}</td>
      <td>{{this.city}}</td>
      <td>{{this.telephone}}</td>
      <td>{{#if this.status}} Activo {{else}} Inactivo {{/if}}</td>
    </tr>
    {{/each}}

  </table>